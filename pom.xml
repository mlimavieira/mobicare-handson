<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.mlimavieira.mcare</groupId>
	<artifactId>handson</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<packaging>pom</packaging>



	<scm>
		<connection>git://github.com/mlimavieira/mobicare-handson</connection>
		<url>https://github.com/mlimavieira/mobicare-handson</url>
	</scm>


	<properties>
		<spring.boot.version>1.2.0.RELEASE</spring.boot.version>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		
	</properties>

	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>org.springframework.boot</groupId>
				<artifactId>spring-boot-dependencies</artifactId>
				<version>1.2.0.RELEASE</version>
				<type>pom</type>
				<scope>import</scope>
			</dependency>

			<dependency>
				<groupId>org.apache.commons</groupId>
				<artifactId>commons-lang3</artifactId>
				<version>3.3.2</version>
			</dependency>
			<dependency>
				<groupId>commons-io</groupId>
				<artifactId>commons-io</artifactId>
				<version>2.4</version>
			</dependency>
			<dependency>
				<groupId>com.amazonaws</groupId>
				<artifactId>aws-java-sdk</artifactId>
				<version>1.9.13</version>
			</dependency>



		</dependencies>
	</dependencyManagement>
	<modules>
		<module>aws-s3</module>
		<module>aws-dynamodb</module>
		<module>aws-sqs</module>
		<module>aws-sns</module>
		<module>aws-elastic-cache</module>
		<module>aws-ses</module>
		<module>aws-cloud-search</module>
	</modules>
</project>