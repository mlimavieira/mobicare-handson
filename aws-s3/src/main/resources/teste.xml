<?xml version="1.0" encoding="UTF-8"?>
<tns:proxyConfiguration xmlns:tns="http://www.mobicare.com.br/proxy/1_0_0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mobicare.com.br/proxy/1_0_0 proxy.xsd ">
	<tns:serviceList>

		<tns:Service enabled="true" public="true" http-method="POST" name="ngt-register" refresh-period="50000" request-uri="/v1/register" remote-url="http://ngtprod-2-env.elasticbeanstalk.com/v1/register" />

		<tns:Service enabled="true" public="true" http-method="POST" name="ngt-v2-register" refresh-period="50000" request-uri="/v2/register" remote-url="http://ngtprod-2-env.elasticbeanstalk.com/v2/register" />


		<tns:Service enabled="true" public="true" http-method="POST" name="ngt-v1-post-msg" refresh-period="50000" request-uri="/v1/notification" remote-url="http://ngtprod-2-env.elasticbeanstalk.com/v1/notification" />


		<tns:Service enabled="true" public="true" http-method="GET" name="ngt-list-msgs" refresh-period="50000" request-uri="/v1/notification" remote-url="http://ngtprod-2-env.elasticbeanstalk.com/v1/notification" />


		<tns:Service enabled="true" public="true" http-method="GET" name="ngt-v2-list-msgs" refresh-period="50000" request-uri="/v2/notification" remote-url="http://ngtprod-2-env.elasticbeanstalk.com/v2/notification" />

		<tns:Service enabled="true" public="true" http-method="GET" name="ngt-getbadge" refresh-period="50000" request-uri="/v1/notification/badge" remote-url="http://ngtprod-2-env.elasticbeanstalk.com/v1/notification/badge" />


		<!-- MCMS -->
		<tns:Service enabled="true" public="false" http-method="GET" name="multiservico-oi-home" refresh-period="50000" request-uri="/rest/" remote-url="http://mcms-core-prod-env.elasticbeanstalk.com/{msisdn}/" />
		<tns:Service enabled="true" public="false" http-method="GET" name="multiservico-oi-list" refresh-period="50000" request-uri="/rest/{target}/list/"
			remote-url="http://mcms-core-prod-env.elasticbeanstalk.com/{msisdn}/{target}/list" />
		<tns:Service enabled="true" public="false" http-method="GET" name="multiservico-oi-detail" refresh-period="50000" request-uri="/rest/{target}/detail/"
			remote-url="http://mcms-core-prod-env.elasticbeanstalk.com/{msisdn}/{target}/detail" />
		<tns:Service enabled="true" public="false" http-method="POST" name="multiservico-oi-buy" refresh-period="50000" request-uri="/rest/{target}/"
			remote-url="http://mcms-core-prod-env.elasticbeanstalk.com/{msisdn}/{target}/" />
		<tns:Service enabled="true" public="false" http-method="GET" name="multiservico-oi-pos-products" refresh-period="50000" request-uri="/rest/products"
			remote-url="http://mcms-core-prod-env.elasticbeanstalk.com/products" />


	</tns:serviceList>

	<tns:providerTypeList>
		<tns:provider enabled="true" name="oingt-default-provider" status="ok" />
		<tns:provider enabled="true" name="provider-multiservicos-oi" status="ok" />
	</tns:providerTypeList>

	<tns:accessTokenList>
		<tns:accessToken challengeType="sms" smsChallengeProvider="ZENVIA" provider-name="oingt-default-provider" value="8AD7FACD-1972-4CEF-862F-13071C94E771" challengeUrl="">
			<tns:AllowedService-name>ngt-register</tns:AllowedService-name>
			<tns:AllowedService-name>ngt-v2-register</tns:AllowedService-name>
			<tns:AllowedService-name>ngt-v1-post-msg</tns:AllowedService-name>
			<tns:AllowedService-name>ngt-list-msgs</tns:AllowedService-name>
			<tns:AllowedService-name>ngt-v2-list-msgs</tns:AllowedService-name>
			<tns:AllowedService-name>ngt-getbadge</tns:AllowedService-name>
		</tns:accessToken>

		<!-- MCMS -->
		<tns:accessToken challengeType="sms" smsChallengeProvider="MUM" challengeUrl="http://mipmcms.mobicare.com.br/services/sms/generateToken/" value="23905613-d3c7-4450-9553-6ead449b1762"
			provider-name="provider-multiservicos-oi">
			<tns:AllowedService-name>multiservico-oi-home</tns:AllowedService-name>
			<tns:AllowedService-name>multiservico-oi-list</tns:AllowedService-name>
			<tns:AllowedService-name>multiservico-oi-detail</tns:AllowedService-name>
			<tns:AllowedService-name>multiservico-oi-buy</tns:AllowedService-name>
			<tns:AllowedService-name>multiservico-oi-pos-products</tns:AllowedService-name>
		</tns:accessToken>

		<!-- MCMS POS -->
		<tns:accessToken challengeType="NONE" smsChallengeProvider="MUM" challengeUrl="http://mipmcms.mobicare.com.br/services/sms/generateToken/" value="62a7de8c-8a29-11e4-b116-123b93f75cba"
			provider-name="provider-multiservicos-oi">
			<tns:AllowedService-name>multiservico-oi-home</tns:AllowedService-name>
			<tns:AllowedService-name>multiservico-oi-list</tns:AllowedService-name>
			<tns:AllowedService-name>multiservico-oi-detail</tns:AllowedService-name>
			<tns:AllowedService-name>multiservico-oi-buy</tns:AllowedService-name>
			<tns:AllowedService-name>multiservico-oi-pos-products</tns:AllowedService-name>
		</tns:accessToken>

	</tns:accessTokenList>
</tns:proxyConfiguration>